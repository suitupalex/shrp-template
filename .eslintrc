{
  "extends": "eslint:recommended"
, "env": {
    "node": true
  , "es6": true
  }
, "rules": {
    "block-scoped-var": 2
  , "consistent-return": 2
  , "curly": 2
  , "dot-notation": 2
  , "eqeqeq": 2
  , "guard-for-in": 2
  , "no-alert": 2
  , "no-caller": 2
  , "no-div-regex": 2
  , "no-else-return": 2
  , "no-eq-null": 2
  , "no-eval": 0
  , "no-extend-native": 2
  , "no-extra-bind": 2
  , "no-implicit-coercion": 2
  , "no-implied-eval": 2
  , "no-iterator": 2
  , "no-multi-spaces": 2
  , "no-multi-str": 2
  , "no-native-reassign": 2
  , "no-new-func": 2
  , "no-new-wrappers": 2
  , "no-new": 2
  , "no-proto": 2
  , "no-return-assign": 2
  , "no-script-url": 2
  , "no-self-compare": 2
  , "no-unused-expressions": 2
  , "no-useless-call": 2
  , "no-useless-concat": 2
  , "no-void": 2
  , "no-with": 2
  , "wrap-iife": 2
  , "no-shadow-restricted-names": 2
  , "no-shadow": 2
  , "no-undef-init": 2
  , "no-unused-vars": 2
  , "handle-callback-err": 2
  , "no-mixed-requires": 2
  , "no-sync": 2
  , "no-underscore-dangle": 0
  , "no-use-before-define": [2, "nofunc"]
  , "quotes": [2, "single", "avoid-escape"]
  , "brace-style": [2, "1tbs", {"allowSingleLine": true}]
  , "comma-style": [2, "first"]
  , "semi": 0
  , "keyword-spacing": 2
  , "indent": [0, 2]
  , "no-console": 0
  , "no-extra-semi": 0
  , "no-floating-decimal": 2
  , "no-multiple-empty-lines": 2
  , "space-before-blocks": 2
  , "object-curly-spacing": 2
  , "array-bracket-spacing": 2
  , "computed-property-spacing": 2
  , "space-in-parens": 2
  , "strict": [2, "global"]
  }
}
